{
    "name": "news-app",
    "private": true,
    "version": "0.0.0",
    "proxy": "http://localhost:5000",
    "type": "module",
    "scripts": {
        "start": "yarn run install-backend && concurrently \"yarn run start-client\" \"yarn run start-server\"",
        "start-service": "yarn run install-backend && concurrently \"yarn run start-client\" \"yarn run start-server-service\"",
        "dev": "yarn run install-backend && concurrently \"yarn run start-client\" \"yarn run start-server\"",
        "start-client": "vite --host",
        "start-server": ". venv/bin/activate && cd src/server && python3 run.py",
        "start-server-service": ". venv/bin/activate && cd src/server && /home/app/News-App/venv/bin/gunicorn --workers 3 --bind unix:webapp.sock -m 007 run:app && python3 src/server/ArticlesFetcher.py",
        "install-backend": "cd src/server && pip install -r requirements.txt",
        "build": "tsc && vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-brands-svg-icons": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@types/react-tooltip": "^4.2.4",
        "axios": "^1.3.4",
        "axios-hooks": "^4.0.0",
        "csurf": "^1.11.0",
        "esbuild": "^0.17.12",
        "jquery": "^3.7.0",
        "js-cookie": "^3.0.5",
        "moment": "^2.29.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.7.1",
        "react-infinite-scroll-component": "^6.1.0",
        "react-loader-spinner": "^5.3.4",
        "react-promise-tracker": "^2.1.1",
        "react-router-dom": "^6.9.0",
        "react-tooltip": "^5.13.1"
    },
    "devDependencies": {
        "@types/jquery": "^3.5.16",
        "@types/js-cookie": "^3.0.3",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@vitejs/plugin-react": "^3.1.0",
        "concurrently": "^7.6.0",
        "sass": "^1.58.3",
        "typescript": "^4.9.3",
        "vite": "^4.1.0",
        "yarn": "^1.22.19"
    },
    "homepage": "./home"
}
